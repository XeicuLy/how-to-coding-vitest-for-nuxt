{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format:check": "prettier . --check",
    "format:fix": "prettier . --write",
    "eslint-rule-check": "npx @eslint/config-inspector@latest",
    "ci:prettier": "prettier --check .",
    "ci:eslint": "eslint .",
    "postinstall": "nuxt prepare",
    "prepare": "husky"
  },
  "dependencies": {
    "nuxt": "3.16.0",
    "vue": "3.5.13",
    "vue-router": "4.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.0",
    "@nuxt/eslint": "1.1.0",
    "@types/node": "22.13.9",
    "@typescript-eslint/parser": "8.26.0",
    "commitizen": "4.3.1",
    "commitlint-config-gitmoji": "2.3.1",
    "cz-customizable": "7.4.0",
    "eslint": "9.22.0",
    "eslint-config-prettier": "10.1.1",
    "globals": "16.0.0",
    "husky": "9.1.7",
    "prettier": "3.5.3",
    "typescript": "5.8.2",
    "vue-eslint-parser": "10.1.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.cts"
    }
  },
  "volta": {
    "node": "22.14.0",
    "pnpm": "9.15.7"
  },
  "packageManager": "pnpm@9.15.7"
}
