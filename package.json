{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format:check": "prettier . --check",
    "format:fix": "prettier . --write",
    "eslint-rule-check": "npx @eslint/config-inspector",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:jsdom": "vitest --ui --environment=jsdom",
    "ci:typecheck": "nuxi typecheck .",
    "ci:vitest": "vitest --coverage --silent",
    "ci:prettier": "prettier --check .",
    "ci:eslint": "eslint .",
    "postinstall": "nuxt prepare",
    "prepare": "husky"
  },
  "dependencies": {
    "@pinia/nuxt": "0.10.1",
    "nuxt": "3.16.0",
    "pinia": "3.0.1",
    "vue": "3.5.13",
    "vue-router": "4.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.0",
    "@eslint/config-inspector": "1.0.2",
    "@nuxt/eslint": "1.1.0",
    "@nuxt/test-utils": "3.17.2",
    "@pinia/testing": "1.0.0",
    "@types/node": "22.13.9",
    "@typescript-eslint/parser": "8.26.0",
    "@vitest/coverage-v8": "3.0.8",
    "@vitest/eslint-plugin": "1.1.36",
    "@vitest/ui": "3.0.8",
    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/test-utils": "2.4.6",
    "commitizen": "4.3.1",
    "commitlint-config-gitmoji": "2.3.1",
    "cz-customizable": "7.4.0",
    "eslint": "9.22.0",
    "eslint-plugin-reactive-value-suffix": "2.0.1",
    "globals": "16.0.0",
    "happy-dom": "17.4.2",
    "husky": "9.1.7",
    "jsdom": "26.0.0",
    "lint-staged": "15.4.3",
    "patch-package": "8.0.0",
    "prettier": "3.5.3",
    "typescript": "5.8.2",
    "vitest": "3.0.8",
    "vue-eslint-parser": "10.1.1",
    "vue-tsc": "2.2.8"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.cts"
    }
  },
  "lint-staged": {
    "**/*.{js,mjs,cjs,ts,cts,mts,vue}": [
      "eslint",
      "prettier --check"
    ],
    "**/*.json": "prettier --check"
  },
  "volta": {
    "node": "22.14.0",
    "pnpm": "9.15.7"
  },
  "packageManager": "pnpm@9.15.7"
}
